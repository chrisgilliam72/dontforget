<?xml version="1.0" encoding="utf-8"?>
<TabbedPage xmlns="http://xamarin.com/schemas/2014/forms"
            xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
            x:Class="DontForget.GoShoppingView"
            xmlns:local="clr-namespace:DontForget"
            BarBackgroundColor="Gainsboro">
    <!--Pages can be added as references or inline-->

    <local:ShoppingListView x:Name="viewShoppingList" IconImageSource="todo.png" />
    <local:ShoppingBillView x:Name="viewShoppingCart"  IconImageSource="shopping.png" />
    <local:ItemMasterListView x:Name="viewMasterList"  IconImageSource="database.png" />
    <local:HelpPageView x:Name="viewHelpPage" IconImageSource="help.png" />
</TabbedPage>
